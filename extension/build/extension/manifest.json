{
  "manifest_version": 2,
  "name": "Phishy",
  "version": "1.0",
  "description": "Phishy extension",
  "background": {
    "scripts": ["alerts_bin.js", "background_bin.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["phishy.js"],
      "css": ["phishy.css"]
    }
  ],
  "web_accessible_resources": ["assets/*", "script.js"],
  "permissions": [
    "https://safebrowsing.google.com/*",
    "activeTab",
    "history",
    "safeBrowsingPrivate",
    "tabs"]
}
